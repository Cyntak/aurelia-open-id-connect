{
  "name": "aurelia-open-id-connect",
  "description": "An Aurelia adapter for the IdentityModel/oidc-client-js.",
  "version": "0.8.0",
  "author": "Shaun Luttin <shaun@bigfont.ca>",
  "main": "dist/amd/es5/open-id-connect/index.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/shaunluttin/aurelia-open-id-connect.git"
  },
  "scripts": {
    "package:clean": "node_modules/.bin/rimraf dist",
    "package:typings": "node_modules/.bin/typings install -y",
    "package:tsc": "node_modules/.bin/tsc --outDir dist/tsc_out",
    "package:babel": "node_modules/.bin/babel dist/tsc_out -d dist/amd",
    "package:html": "node_modules/.bin/cpx src/open-id-connect/**/*.html dist/amd",
    "package": "npm run package:clean && npm run package:typings && npm run package:tsc && npm run package:babel && npm run package:html"
  },
  "jspm": {
    "main": "dist/amd/es5/open-id-connect/index.js",
    "registry": "npm",
    "jspmPackage": true,
    "format": "amd",
    "files": [
      "src",
      "dist"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "aurelia-cli": "^0.19.0",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "browser-sync": "^2.15.0",
    "cpx": "^1.5.0",
    "event-stream": "^3.3.4",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-changed-in-place": "^2.0.3",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.13.6",
    "merge2": "^1.0.2",
    "ncp": "^2.0.0",
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1",
    "typescript": "^2.0.0",
    "typings": "^1.3.3"
  },
  "peerDependencies": {
    "aurelia-fetch-client": "^1.0.0",
    "aurelia-framework": "^1.0.2",
    "aurelia-router": "^1.0.2",
    "oidc-client": "^1.0.0"
  }
}
