{
  "name": "aurelia-open-id-connect",
  "description": "An Aurelia adapter for the IdentityModel/oidc-client-js.",
  "version": "0.10.2",
  "author": "Shaun Luttin <shaun@bigfont.ca>",
  "main": "dist/amd/index.js",
  "types": "dist/tsc_out/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/shaunluttin/aurelia-open-id-connect.git"
  },
  "scripts": {
    "tslint": "node_modules/.bin/tslint --config tslint.json --project tsconfig.json",
    "package:clean": "node_modules/.bin/rimraf dist",
    "package:tsc": "node_modules/.bin/tsc --outDir dist/tsc_out",
    "package:babel": "node_modules/.bin/babel dist/tsc_out -d dist/amd",
    "package:html": "node_modules/.bin/cpx src/**/*.html dist/amd",
    "package": "npm run package:clean && npm run package:tsc && npm run package:babel && npm run package:html"
  },
  "jspm": {
    "main": "index.js",
    "directories": {
      "lib": "dist/amd"
    },
    "format": "amd",
    "registry": "npm"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "tslint": "^3.15.1",
    "tslint-microsoft-contrib": "^2.0.13",
    "typescript": "^2.0.0"
  },
  "dependencies": {
    "@types/whatwg-fetch": "0.0.32",
    "aurelia-fetch-client": "^1.0.0",
    "aurelia-framework": "^1.0.6",
    "aurelia-router": "^1.0.6",
    "cpx": "^1.5.0",
    "jwt-decode": "^2.1.0",
    "oidc-client": "github:shaunluttin/oidc-client-js#master"
  }
}
