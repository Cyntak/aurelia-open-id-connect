{
  "name": "aurelia-oidc-client",
  "description": "An Aurelia adapter for the IdentityModel/oidc-client-js.",
  "version": "0.1.0",
  "author": "Shaun Luttin <shaun@bigfont.ca>",
  "main": "dist/amd/es5/open-id/open-id.js",
  "typings": "dist/amd/es6/open-id/open-id.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/shaunluttin/aurelia-oidc-client.git"
  },
  "scripts": {
    "prepublish:clean": "rimraf dist",
    "prepublish:typings": "node_modules/.bin/typings install -y",
    "prepublish": "npm run prepublish:typings && npm run prepublish:clean",
    "publish:tsc": "node_modules/.bin/tsc --outDir dist/amd/es6",
    "publish:babel": "node_modules/.bin/babel dist/amd/es6 -d dist/amd/es5",
    "publish": "npm run publish:tsc && npm run publish:babel"
  },
  "jspm": {
    "main": "dist/amd/es5/open-id/open-id",
    "registry": "npm",
    "jspmPackage": true,
    "format": "amd",
    "files": [
      "src",
      "dist"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "typescript": "^2.0.0",
    "typings": "^1.3.3"
  },
  "dependencies": {
    "aurelia-fetch-client": "^1.0.0",
    "aurelia-framework": "^1.0.2",
    "aurelia-router": "^1.0.2"
  }
}
