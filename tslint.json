{
  "defaultSeverity": "error",
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "callable-types": false,
    "space-before-function-paren": false,
    "no-implicit-dependencies": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ]
  }
}
